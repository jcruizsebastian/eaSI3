{
  "compilerOptions": {
    "paths": { "*": [ "types/*" ] },
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": [ "webpack-env" ]
  },
  "compileOnSave": true,
  "exclude": [
      "bin",
      "node_modules"
  ]
}
